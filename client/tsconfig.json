{
  "compilerOptions": {
    "module": "umd",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "es6",
      "es2015.promise",
      "dom"
    ],
    "isolatedModules": false,
    "declaration": true,
    "moduleResolution": "node",
    "noLib": false,
    "noEmitOnError": false
  },
  "exclude": [
    "node_modules"
  ],
  "files": [
    "./module.ts",
    "./Auth.ts",
    "./LoginScreen.ts",
    "./Skin/LoginScreen.ts"
  ]
}